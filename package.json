{
  "name": "baaday-back",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --no-notify --respawn src/server.ts",
    "lint:fix": "eslint --fix . --ext ts,tsx",
    "test": "jest",
    "typeorm": "typeorm-ts-node-commonjs",
    "m:create": "typeorm migration:create",
    "m:run": "npx typeorm-ts-node-commonjs migration:run -d src/dataSource/connection.ts",
    "m:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/dataSource/connection.ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^8.5.1",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/node": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.3",
    "pg": "^8.7.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.2.2"
  }
}
